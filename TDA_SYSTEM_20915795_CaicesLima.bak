#lang racket
(require racket/date)

(define make-system (lambda
                        (system-name
                         current-user
                         current-path
                         users
                         drives
                         system-date)
                        (list system-name
                              current-user
                              current-path
                              users
                              drives
                              system-date)))

(define system (lambda (name)
                 (make-system name
                              null ;user
                              null ;path
                              null ;users
                              null ;drives
                              current-date) ;fecha
                 ))